<Window x:Class="IPIO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IPIO"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button Content="Button" HorizontalAlignment="Left" Margin="60,0,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <Image x:Name="ImageBefore" Height="125" Width="154"/>
        <Image x:Name="ImageAfter"  Height="125" Width="154"/>
        <Label Content="Label"/>
    </StackPanel>


</Window>
